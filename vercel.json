{
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/wsgi.py" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
